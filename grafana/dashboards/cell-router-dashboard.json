{
  "dashboard": {
    "id": null,
    "title": "Cell Router Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "Requests per Second",
        "targets": [
          {
            "expr": "rate(requests_total[1m])",
            "legendFormat": "requests",
            "refId": "A"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Request Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, sum(rate(request_latency_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.9, sum(rate(request_latency_bucket[5m])) by (le))",
            "legendFormat": "p90",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(request_latency_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(1, sum(rate(request_latency_bucket[5m])) by (le))",
            "legendFormat": "p100",
            "refId": "D"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Successful Requests",
        "targets": [
          {
            "expr": "rate(requests_success_total[1m])",
            "legendFormat": "success",
            "refId": "A"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Failed Requests",
        "targets": [
          {
            "expr": "rate(requests_error_total[1m])",
            "legendFormat": "errors",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
